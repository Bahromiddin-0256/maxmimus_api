}

model about_about {
  id             BigInt   @id @default(autoincrement())
  created_at     DateTime @db.Timestamptz(6)
  updated_at     DateTime @db.Timestamptz(6)
  description    String
  description_uz String?
  description_sr String?
  description_ru String?
}

